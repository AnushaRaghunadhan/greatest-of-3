#include <stdio.h>

int main()
{
    float a,b,c;
    printf("enter the three numbers");
    scanf("%f%f%f",&a,&b,&c);
    if(a>=b&&a>=c)
      printf("largest of three numbers is=%f ",a);
    if(b>=a&&b>=c)
      printf("largest of three numbers is=%f ",b);
       if(c>=a&&c>=b)
      printf("largest of three numbers is=%f ",c);
      return(0);
}

